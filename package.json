{
  "name": "node-app-tdd",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=DEVELOPMENT./node_modules/.bin/nodemon ./node_modules/.bin/babel-node ./src/bin/www",
